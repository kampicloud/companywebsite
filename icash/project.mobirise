                "height": "~\"calc(100% - 8px)\"",
                "border-radius": "50%",
                "background": "linear-gradient(-51deg, rgb(249, 154, 120) 0%, rgb(136, 52, 204) 20%, rgb(55, 189, 209) 42%, rgb(212, 234, 62) 62%, rgb(248, 102, 157) 83%, rgb(252, 93, 149) 91%, rgb(255, 83, 140) 100%)",
