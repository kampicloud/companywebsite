              }
            },
            ".embla": {
              "position": "relative",
              "width": "100%",
              "& when (@arrows)": {
                "padding-bottom": "5rem"
              }
            },
            ".embla__viewport": {
              "overflow": "hidden",
              "width": "100%",
              "margin-right": "1rem"
            },
            ".embla__viewport.is-draggable": {
              "cursor": "grab"
            },
            ".embla__viewport.is-dragging": {
              "cursor": "grabbing"
            },
            ".embla__container": {
              "display": "flex",
              "user-select": "none",
              "-webkit-touch-callout": "none",
              "-khtml-user-select": "none",
              "-webkit-tap-highlight-color": "transparent"
            },
            ".mbr-section-title": {
              "color": "#232323"
            },
            ".item-title": {
              "color": "#192227"
            },
            ".mbr-text": {
